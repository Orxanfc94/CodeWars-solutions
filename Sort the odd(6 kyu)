 function sortArray(array) {
      // Return a sorted array.
      let f=[]
      let x=[...array]
      let y=[...array].sort((a,b)=>a-b).filter(a=>a%2!=0)
   for(let i=0;i<x.length;i++){

      if(x[i]%2!=0){
        
           f.push(y.shift())
        }
 else if(x[i]%2==0){
      f.push(x[i])
 }

   }
   
  return f
    }
